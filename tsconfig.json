{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "es6"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "noImplicitThis": true,
    "strictNullChecks": true
  },
  "files": [
      "src/components/Metrics/Graph.tsx",
      "src/components/Metrics/Metrics.tsx",
      "src/containers/DeckList/DeckListEntry.tsx",
      "src/containers/DeckList/DeckListEntries.tsx",
      "src/containers/Search/Search.tsx",
      "src/reducers/CardCountReducer.ts"
  ],
  "include": [
      "src/components/Card"
  ]
}
